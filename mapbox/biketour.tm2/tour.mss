#track {
  line-color: #d92e26;
  line-width: 4;   
  line-join: round;
  line-cap: round;
}

#timeline[marker-symbol='campsite'] {
  marker-file: url(maki/campsite.svg);
  marker-fill: #810e09;
  marker-width: 25;
  marker-allow-overlap: true;
}

#track[marker-symbol='embassy'] {
  marker-file: url(maki/embassy.svg);
  marker-fill: #810e09;
  marker-width: 35;
  marker-allow-overlap: true;
}